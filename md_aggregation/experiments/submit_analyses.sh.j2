#!/bin/bash

{% set analysis_path = project_path + '/ClusterAnalysis/ClusterAnalysis.py' %}
aslurm -c {{ config }} \
{% for info in infos %}
    cmd "python {{ analysis_path }} -hb -solv '{{ info["solvent"] }}' -ex '{{ info["exclude"] }}' -e {{ info["end"] }} -traj '{{ info["trajectory_path"] }}' -top '{{ info["topology_path"] }}'" \
{% endfor %}